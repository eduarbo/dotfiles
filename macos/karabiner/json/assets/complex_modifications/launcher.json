{
  "title": "Launcher",
  "rules": [
    {
      "description": "Launch apps by right option+letters",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"/System/Applications/Calendar.app\""
            }
          ],
          "description": "from right_option+a to shell command \"open -a \"/System/Applications/Calendar.app\"\""
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_option"
              ],
              "optional": [
                "caps_lock"
              ]
