  "rules": [
    {
      "description": "Civ V layer: Thumbs cluster",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command"
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "left_shift"
              ]
            }
          ],
          "description": "right_command to right_command, send left_shift+return_or_enter on tap",
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.aspyr.civ5xp.steam$"
              ]
            }
          ]
        }
      ]
