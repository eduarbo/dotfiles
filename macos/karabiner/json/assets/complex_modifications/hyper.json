{
  "title": "Hyper layer",
  "rules": [
    {
      "description": "Hyper keys for quick access to the bindings I use most often",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "m",
              "modifiers": [
                "right_shift",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+w to right_shift+right_command+m"
        },
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "i",
              "modifiers": [
                "right_alt",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+e to right_alt+right_command+i"
        },
        {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": [
                "right_shift",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+r to right_shift+right_command+c"
        },
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "right_shift",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+x to right_shift+right_command+3"
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "4",
              "modifiers": [
                "right_shift",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+c to right_shift+right_command+4"
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "command",
                "option",
                "control",
                "shift"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": [
                "right_shift",
                "right_command"
              ]
            }
          ],
          "description": "from command+option+control+shift+v to right_shift+right_command+5"
        }
      ]
    }
  ]
}