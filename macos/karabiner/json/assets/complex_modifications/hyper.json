{
  "title": "Main layer",
  "rules": [
    {
      "description": "HYPER layer",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": []
            }
          },
          "to": [
            {
              "key_code": "right_shift",
              "modifiers": [
                "option",
                "command",
                "control"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "e",
              "modifiers": [
                "right_option"
              ]
            }
          ],
          "description": "left_command+spacebar to option+command+control+right_shift, send right_option+e on tap"
        }
      ]
    }
  ]
}